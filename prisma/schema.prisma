datasource db {
  provider = "sqlite"
  url      = "file:../data/wallets.db"
}

generator client {
  provider = "prisma-client-js"
}

model Wallet {
    id Int @id @default(autoincrement())
    name String @unique
    currency String
    balance Float
    todayBalanceChange Float @default(0)
    createdAt DateTime @default(now())
}
